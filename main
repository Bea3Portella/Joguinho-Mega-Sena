print('-'*30)
print(f"{'JOGO DA MEGA SENA': ^30}")
print('-'*30)
count_game = int(input('Quantos jogos vocÃª quer que eu sorteie? '))
print('-='*3, end=' ')
print(f'SORTEANDO {count_game} JOGOS', end=' ')
print('-='*3)
game = [0, 0, 0, 0, 0, 0]
count = 0
import time
import random
for t in range(count_game):
    for i, j in enumerate(game):
        game[i] = (random.randint(0, 60))
    count += 1
    time.sleep(1)
    print(f'Jogo {t+1}: {game}')







